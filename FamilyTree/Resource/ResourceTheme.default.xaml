    <!--Detail-->
    <SolidColorBrush x:Key="btnAdd_Detail">#02EE5B</SolidColorBrush>
    <!--btn thêm quan hệ-->
    <SolidColorBrush x:Key="zoneAdd_Detail">#FFFAFA</SolidColorBrush>
    <!--Nền vùng thêm quan hệ-->
    <SolidColorBrush x:Key="bg_Detail">#FFFAFA</SolidColorBrush>
    <!--Nền-->

    <!--FamilyData-->
    <SolidColorBrush x:Key="bg_FamilyData">#fafafa</SolidColorBrush>

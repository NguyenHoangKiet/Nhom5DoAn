    <SolidColorBrush x:Key="label_FamilyData">AliceBlue</SolidColorBrush>
    <!--Nền của thanh chứa họ tên, ngày sinh vv-->
    <!--Nền lable nhãn màu tím chưa cài-->

    <!--NewUser-->
    <SolidColorBrush x:Key="bg_NewUser">#fafafa</SolidColorBrush>
    <!--Nền-->

    <!--FamilyData-->
    <SolidColorBrush x:Key="bgListView">AliceBlue</SolidColorBrush>

    <!--MoreDetail-->
    <SolidColorBrush x:Key="bg_MoreDetail">LightBlue</SolidColorBrush>
    <!--Nền-->
    <SolidColorBrush x:Key="btnSave_MoreDetail">#02EE5B</SolidColorBrush>
    <!--btn lưu-->

    <!--<SolidColorBrush x:Key="bgWhite">white</SolidColorBrush>
    <SolidColorBrush x:Key="foregroundTextDark">black</SolidColorBrush>
    <SolidColorBrush x:Key="foregroundTextLight">white</SolidColorBrush>-->


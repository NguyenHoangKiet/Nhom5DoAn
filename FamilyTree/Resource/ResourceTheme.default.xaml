<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Sài chung tất cả các WD -->
    <SolidColorBrush x:Key="button">#673AB7</SolidColorBrush>
    <SolidColorBrush x:Key="controlbar">#512DA8</SolidColorBrush>
    <SolidColorBrush x:Key="border">#512DA8</SolidColorBrush>

    <!--background nút ở Ctrolbar-->
    <SolidColorBrush x:Key="btn_Exit">red</SolidColorBrush>
    <SolidColorBrush x:Key="btn_Resize">#673AB7</SolidColorBrush>

    <!--WelcomeWD Chưa cần cài-->

    <!--MainWD-->
    <SolidColorBrush x:Key="bg_MainWD1">#512DA8</SolidColorBrush>
    <!--Background này ko cần quan tâm-->
    <SolidColorBrush x:Key="bg_MainWD2">#8C82FC</SolidColorBrush>

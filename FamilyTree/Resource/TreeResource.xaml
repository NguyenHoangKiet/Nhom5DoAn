    <Style x:Key="DiagramToolTip" TargetType="ToolTip">
        <Setter Property="OverridesDefaultStyle" Value="true" />
        <Setter Property="HasDropShadow" Value="True" />
        <Setter Property="Foreground" Value="#FFFFFFFF" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToolTip">
                    <Border
                        Name="Border"
                        Background="{DynamicResource DiagramToolTipBackgroundBrush}"
                        BorderBrush="#FFFFFFFF"
                        BorderThickness="1,1,1,1"
                        CornerRadius="10,10,10,10">
                        <DockPanel>
                            <StackPanel Margin="0,0,5,10">
                                <Border
                                    x:Name="AvatarPhotoBorder"
                                    Margin="10,5,10,0"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Top"
                                    Background="#02000000"
                                    BorderBrush="#02747474"
                                    BorderThickness="1,1,1,1">
                                    <Border.BitmapEffect>
                                        <DropShadowBitmapEffect ShadowDepth="7" Softness="0.75" />
                                    </Border.BitmapEffect>
                                    <Image
                                        x:Name="Avatar"
                                        MaxWidth="90"
                                        MaxHeight="85"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Top"
                                        Source="{Binding Path=Person.Avatar, Converter={StaticResource ImageConverter}, Mode=OneWay}"
                                        Stretch="Uniform"
                                        StretchDirection="DownOnly" />
                                </Border>
                                <Border
                                    x:Name="NoPhotoBorder"
                                    Margin="10,5,10,0"
                                    Background="#02FFFFFF"
                                    BorderBrush="#FFFFFFFF"
                                    Visibility="{Binding Path=Person.HasAvatar, Converter={StaticResource ComposingConverter}, Mode=Default}">
                                    <Rectangle
                                        x:Name="NoPhotoRectangle"
                                        Width="70"
                                        Height="85"
                                        Fill="{DynamicResource NoPhotoBrush}"
                                        Stroke="{x:Null}">
                                        <Rectangle.BitmapEffect>
                                            <DropShadowBitmapEffect ShadowDepth="7" Softness="0.75" />
                                        </Rectangle.BitmapEffect>
                                    </Rectangle>
                                </Border>
                            </StackPanel>
                            <StackPanel MaxWidth="170" Margin="0,0,5,10">
                                <ContentPresenter Margin="0,10,10,5" />
                                <TextBlock
                                    x:Name="ParentsDatePanel"
                                    Margin="0,0,0,5"
                                    TextWrapping="Wrap">
                                    <Run local:BindableExtender.BindableText="{Binding Path=Person.ParentRelationshipText}" /><Run Text=" của " /><Run local:BindableExtender.BindableText="{Binding Path=Person.ParentsText}" />
                                </TextBlock>
                                <TextBlock
                                    x:Name="SiblingsDatePanel"
                                    Margin="0,0,0,5"
                                    TextWrapping="Wrap">
                                    <Run local:BindableExtender.BindableText="{Binding Path=Person.SiblingRelationshipText}" /><Run Text=" của " /><Run local:BindableExtender.BindableText="{Binding Path=Person.SiblingsText}" />
                                </TextBlock>
                                <TextBlock
                                    x:Name="SpousesDatePanel"
                                    Margin="0,0,0,5"
                                    TextWrapping="Wrap">
                                    <Run local:BindableExtender.BindableText="{Binding Path=Person.SpouseRelationshipText}" /><Run Text=" của " /><Run local:BindableExtender.BindableText="{Binding Path=Person.SpousesText}" />
                                </TextBlock>
                                <TextBlock
                                    x:Name="ChildrenDatePanel"
                                    Margin="0,0,0,5"
                                    TextWrapping="Wrap">
                                    <Run local:BindableExtender.BindableText="{Binding Path=Person.ChildRelationshipText}" /><Run Text=" của " /><Run local:BindableExtender.BindableText="{Binding Path=Person.ChildrenText}" />
                                </TextBlock>
                            </StackPanel>
                        </DockPanel>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="HasDropShadow" Value="true">
                            <Setter TargetName="Border" Property="CornerRadius" Value="4" />
                            <Setter TargetName="Border" Property="SnapsToDevicePixels" Value="true" />
                        </Trigger>
                        <DataTrigger Binding="{Binding Path=Person.Parents.Count}" Value="0">
                            <Setter TargetName="ParentsDatePanel" Property="Visibility" Value="Collapsed" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Person.Siblings.Count}" Value="0">
                            <Setter TargetName="SiblingsDatePanel" Property="Visibility" Value="Collapsed" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Person.Spouses.Count}" Value="0">
                            <Setter TargetName="SpousesDatePanel" Property="Visibility" Value="Collapsed" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Person.Children.Count}" Value="0">
                            <Setter TargetName="ChildrenDatePanel" Property="Visibility" Value="Collapsed" />
                        </DataTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

